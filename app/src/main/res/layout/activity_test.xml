<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center"
	>
	<Button
		android:id="@+id/btnProgressTestClick"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Activity ProgressDialog Click !"
		/>
	<Button
		android:id="@+id/btnOneBtnTestClick"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Activity OneBtnDialog Click !"
		/>
	<Button
		android:id="@+id/btnTwoBtnTestClick"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Activity TwoBtnDialog Click !"
		/>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		>
		<fragment class="kr.co.bluesky05.bsdialogfactory.TestFragment"
		          android:id="@+id/fgmCourse"
		          android:layout_width="match_parent"
		          android:layout_height="match_parent"
			/>
	</RelativeLayout>
</LinearLayout>